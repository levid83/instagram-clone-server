{
  "name": "instagram-clone-server",
  "version": "0.0.0",
  "private": true,
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "retainLines": true
  },
  "jest": {
    "verbose": true
  },
  "scripts": {
    "start": "nodemon --exec babel-node -r dotenv/config server.js",
    "dev": "npm run start",
    "build": "babel src -d dist",
    "test": "jest"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "bcryptjs": "^2.4.3",
    "dotenv": "^6.2.0",
    "express": "^4.17.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^5.9.7",
    "nodemailer": "^6.4.6"
  },
  "devDependencies": {
    "@babel/core": "^7.6.0",
    "@babel/node": "^7.6.1",
    "@babel/preset-env": "^7.6.0",
    "@babel/register": "^7.6.0",
    "@types/jest": "^24.0.18",
    "acorn": "^6.1.0",
    "babel-jest": "^24.9.0",
    "jest": "^24.9.0",
    "nodemon": "^1.18.10"
  }
}
